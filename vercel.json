{
    "version": 2,
    "builds": [
      { "src": "frontend/build", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/todos", "dest": "todo_app/wsgi.py" },
      { "src": "/(.*)", "dest": "frontend/build/index.html" }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "todo_project.settings"
    }
  }
  