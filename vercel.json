{
    "version": 2,
    "builds": [
      {
        "src": "todo_project/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "frontend/build",
        "use": "@vercel/static",
        "config": {
          "trailingSlash": true
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/frontend/build/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "todo_project/wsgi.py"
      }
    ]
  }
  